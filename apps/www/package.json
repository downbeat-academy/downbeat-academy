{
	"name": "www",
	"version": "4.4.0",
	"private": true,
	"description": "Primary web UI for Downbeat Academy",
	"author": "Jory Tindall <jory@downbeatacademy.com>",
	"license": "UNLICENSED",
	"keywords": [
		"nextjs",
		"sanity",
		"music",
		"education"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/downbeat-academy/downbeat-academy.git"
	},
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"postbuild": "next-sitemap --config next-sitemap.config.js",
		"start": "next start",
		"lint": "next lint",
		"cypress": "cypress open",
		"cypress:install": "cypress install",
		"cypress:run": "cypress run",
		"cypress:run:chrome": "cypress run --browser chrome",
		"cypress:run:firefox": "cypress run --browser firefox",
		"cypress:run:ci": "cypress run --browser chrome --headless --record",
		"cypress:run:auth": "cypress run --spec 'cypress/e2e/auth/**'",
		"cypress:run:routes": "cypress run --spec 'cypress/e2e/routes/**'",
		"cypress:run:forms": "cypress run --spec 'cypress/e2e/forms/**'",
		"cypress:run:a11y": "cypress run --spec 'cypress/e2e/accessibility/**'",
		"cypress:run:a11y:only": "CYPRESS_SKIP_OVERRIDE=true cypress run --spec 'cypress/e2e/accessibility/**'",
		"cypress:run:journeys": "cypress run --spec 'cypress/e2e/journeys/**'",
		"test:e2e": "start-server-and-test dev http://localhost:3000 cypress:run",
		"test:e2e:ci": "start-server-and-test start http://localhost:3000 cypress:run:ci",
		"analyze": "ANALYZE=true pnpm build",
		"db:push": "drizzle-kit push --config=drizzle-auth.config.ts",
		"db:migrate": "drizzle-kit migrate --config=drizzle-auth.config.ts",
		"db:seed:test": "tsx cypress/support/db-seed.ts"
	},
	"dependencies": {
		"@hookform/resolvers": "^3.10.0",
		"@neondatabase/serverless": "^0.10.4",
		"@next/bundle-analyzer": "^15.1.4",
		"@portabletext/react": "^3.2.0",
		"@radix-ui/react-dialog": "^1.1.4",
		"@radix-ui/react-dropdown-menu": "^2.1.4",
		"@radix-ui/react-hover-card": "^1.1.4",
		"@radix-ui/react-separator": "^1.1.1",
		"@radix-ui/react-switch": "^1.1.2",
		"@radix-ui/react-tabs": "^1.1.2",
		"@radix-ui/react-toast": "^1.2.4",
		"@radix-ui/react-tooltip": "1.1.6",
		"@sanity/asset-utils": "^2.2.1",
		"@sanity/client": "^7.8.1",
		"@sanity/image-url": "^1.1.0",
		"@sentry/nextjs": "^9.43.0",
		"@vercel/og": "^0.6.4",
		"better-auth": "^1.3.4",
		"boring-avatars": "^1.11.2",
		"cadence-core": "workspace:^",
		"cadence-icons": "workspace:^",
		"cadence-tokens": "workspace:^",
		"classnames": "^2.5.1",
		"date-fns": "^4.1.0",
		"dotenv": "^16.4.7",
		"drizzle-orm": "^0.44.4",
		"eslint": "^9.32.0",
		"eslint-config-next": "^15.4.5",
		"fathom-client": "^3.7.2",
		"next": "^15.4.5",
		"next-sanity": "^9.12.3",
		"next-sitemap": "^4.2.3",
		"nuqs": "^2.3.1",
		"opensheetmusicdisplay": "^1.8.9",
		"pg": "^8.14.0",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"react-hook-form": "^7.54.2",
		"react-loading-skeleton": "^3.5.0",
		"resend": "^4.7.0",
		"server-only": "^0.0.1",
		"suspend-react": "^0.1.3",
		"typeface-favorit": "workspace:^",
		"typeface-tiempos-text": "workspace:^",
		"zod": "^3.24.1"
	},
	"devDependencies": {
		"@types/bcryptjs": "^3.0.0",
		"@types/node": "^22.10.6",
		"@types/pg": "^8.11.11",
		"@types/react": "^18.3.21",
		"@types/react-dom": "^18.3.7",
		"bcryptjs": "^3.0.2",
		"cypress": "^13.17.0",
		"cypress-axe": "^1.6.0",
		"drizzle-kit": "^0.31.4",
		"ignore-loader": "^0.1.2",
		"nanoid": "^5.1.5",
		"postgres": "^3.4.7",
		"start-server-and-test": "^2.0.13",
		"tsx": "^4.20.3",
		"typescript": "^5.7.3"
	}
}