{
	"$schema": "https://turbo.build/schema.json",
	"pipeline": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": ["dist/**"]
		},
		"test": {
			"dependsOn": ["build"],
			"outputs": []
		},
		"lint": {
			"outputs": []
		},
		"dev": {
			"cache": false
		},
		"storybook": {
			"cache": false
		},
		"build:storybook": {
			"outputs": ["storybook-static/**"]
		}
	}
}