{
	"$schema": "https://turbo.build/schema.json",
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": [".next/**", "!.next/cache/**", "dist/**"],
			"inputs": ["$TURBO_DEFAULT$", ".env", ".env.local"]
		},
		"test": {
			"dependsOn": ["build"],
			"outputs": []
		},
		"lint": {
			"outputs": []
		},
		"dev": {
			"dependsOn": ["^build"],
			"cache": false,
			"persistent": true
		},
		"storybook": {
			"dependsOn": ["^build"],
			"cache": false,
			"persistent": true
		},
		"build:storybook": {
			"dependsOn": ["^build"],
			"outputs": ["storybook-static/**"]
		}
	}
}