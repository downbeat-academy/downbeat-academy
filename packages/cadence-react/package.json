  },
  "dependencies": {
    "boring-avatars": "^1.7.0",
    "cadence-tokens": "workspace:^2.0.1",
    "classnames": "^2.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.0",
    "typeface-favorit": "workspace:^1.0.2",
    "typeface-tiempos-text": "workspace:^1.0.2"
  },
  "devDependencies": {
    "@testing-library/react": "^13.4.0",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@vitejs/plugin-react": "^3.0.1",
    "sass": "^1.57.1",
    "typescript": "^4.9.3",
    "typescript-plugin-css-modules": "^4.1.1",
    "vite": "^4.0.4",
    "vite-plugin-css-injected-by-js": "^2.4.0",
    "vite-plugin-dts": "^1.7.1",
    "vite-tsconfig-paths": "^4.0.5",
    "vitest": "^0.26.3"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "files": [
    "dist"
