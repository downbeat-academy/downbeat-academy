{
  "name": "cadence-icons",
  "version": "1.6.0",
  "author": "Jory Tindall <jory@downbeatacademy.com>",
  "type": "module",
  "license": "UNLICENSED",
  "scripts": {
    "dev": "vite",
    "ensure-icons": "node scripts/ensure-icons.js",
    "prebuild": "pnpm ensure-icons",
    "build": "tsc --noEmit && vite build",
    "postbuild": "echo 'Icons package built successfully'",
    "preview": "vite preview",
    "clean": "rimraf src/components",
    "svgr": "svgr --config-file .svgrrc.json --out-dir src/components src/assets",
    "icons:build": "pnpm clean && pnpm svgr",
    "test": "vitest"
  },
  "dependencies": {
    "classnames": "^2.5.1"
  },
  "peerDependencies": {
    "react": "^18.2.0 || ^19.0.0",
    "react-dom": "^18.2.0 || ^19.0.0"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/cadence-icons.umd.js",
  "module": "./dist/cadence-icons.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/cadence-icons.es.js",
      "require": "./dist/cadence-icons.umd.js",
      "default": "./dist/cadence-icons.es.js"
    }
  },
  "devDependencies": {
    "@svgr/cli": "^8.1.0",
    "@testing-library/react": "^16.1.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.7.0",
    "postcss": "^8.5.1",
    "postcss-nesting": "^13.0.1",
    "rimraf": "^6.0.1",
    "typescript": "^5.7.3",
    "vite": "^7.0.6",
    "vite-plugin-css-injected-by-js": "^3.5.2",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-svgr": "^4.3.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^2.1.8"
  }
}