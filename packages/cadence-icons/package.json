{
  "name": "@downbeat-academy/cadence-icons",
  "private": false,
  "version": "1.0.1",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "tsc && pnpm icons:build && vite build",
    "preview": "vite preview",
    "clean": "rimraf src/components",
    "svgr": "svgr --config-file .svgrrc.json --out-dir src/components src/assets",
    "icons:build": "pnpm clean && pnpm svgr",
    "test": "vitest"
  },
  "dependencies": {
    "classnames": "^2.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@svgr/cli": "^6.5.1",
    "@testing-library/react": "^13.4.0",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@vitejs/plugin-react": "^3.0.0",
    "postcss": "^8.4.20",
    "postcss-nesting": "^10.2.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.9.3",
    "vite": "^4.0.0",
    "vite-plugin-css-injected-by-js": "^2.2.0",
    "vite-plugin-dts": "^1.7.1",
    "vite-plugin-svgr": "^2.4.0",
    "vite-tsconfig-paths": "^4.0.3",
    "vitest": "^0.26.1"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/cadence-icons.umd.js",
  "module": "./dist/cadence-icons.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/cadence-icons.es.js",
      "require": "./dist/cadence-icons.umd.js"
    }
  }
}